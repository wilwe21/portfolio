  box-shadow: inset 0 0 0 2px rgba(205, 214, 244, 0);
  border: 2px solid #fff000;
  border-radius: 7px;
  text-shadow: 2px 2px 0px #000000;
  background-repeat: no-repeat;
  background-position: center;
  padding: 7px;
  margin: 10px;
  grid-area: dots;
  background-image: url("../images/dots.png");
  background-size: 300px;
}
.dots:focus {
  box-shadow: inset 0 0 0 2px #fff000;
  background-color: rgba(205, 214, 244, 0.154);
  color: #f7f8fd;
}
.dots:hover {
  box-shadow: inset 0 0 0 2px rgba(205, 214, 244, 0);
  background-color: rgba(205, 214, 244, 0.154);
  color: #f7f8fd;
}
.dots:active, .dots.on, .dots.active, .dots:checked {
  box-shadow: inset 0 0 0 2px rgba(205, 214, 244, 0);
  background-image: linear-gradient(to right, #fff000, #999000);
  background-color: #fff000;
  color: #181825;
}
.dots:active:hover, .dots.on:hover, .dots.active:hover, .dots:checked:hover {
  box-shadow: inset 0 0 0 2px rgba(205, 214, 244, 0), inset 0 0 0 99px rgba(205, 214, 244, 0.154);
}
.dots:disabled {
  box-shadow: none;
  background-color: transparent;
  color: rgba(205, 214, 244, 0.3);
}

